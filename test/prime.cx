{
    const int maxn = 100;

    function prime
    {
        int i;
        int j;
        bool flag;
        i = 2;
        write 2;
        while(i < maxn) {
            i++;
            flag = false;
            j = 2;

            while(i >= j * j) {
                if(0 == i % j) {
                    flag = true;
                    j = i;
                };
                j++;
            };

            if(flag == 0) { // 此处暂时不能用 false, 因为 false 属于 another_expr, 不属于 some_expr
                write(i);
            };
        };
    }

    call prime;
}