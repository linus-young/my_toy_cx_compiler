{
    function lcm
    {
        int x;
        int y;
        int mul;
        int r;
        int ans;

        read x;
        read y;

        mul = x * y;
        while(y != 0) {
            r = x % y;
            x = y;
            y = r;
        };

        ans = mul / x;
        write ans;

    }

    call lcm;
}


